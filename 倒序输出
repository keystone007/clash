#include<stdio.h>

int main(){
    int num;
        scanf("%d",&num);
        int a=num/100;
        int b=(num-100*a)/10;
        int c=num-100*a-10*b;
    if(c==0){
        int d=10*b+a;
        printf("%d\n",d);}
    if(b==0,c==0){
        printf("%d\n",a);}
    else {int e=100*c+10*b+a;
        printf("%d\n",e);}
        return 0;
}
